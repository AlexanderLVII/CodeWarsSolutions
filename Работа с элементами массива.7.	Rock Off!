//https://www.codewars.com/kata/5b097da6c3323ac067000036/train/javascript
function solve(a, b) {
  let sa = 0, sb = 0;
  for (let i = 0; i <= 2;i++){
     if (a[i] > b[i]) sa++;
     else if (a[i] < b[i]) sb++
  }
  return (sa +', ' + sb + ': ' ) + (sa > sb ?   'Alice made "Kurt" proud!' : sa < sb ? 'Bob made "Jeff" proud!' :'that looks like a "draw"! Rock on!' )
}
