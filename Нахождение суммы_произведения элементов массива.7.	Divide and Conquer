// https://www.codewars.com/kata/57eaec5608fed543d6000021/train/javascript
function divCon(x){
  let sum = 0;                           
  let sumS = 0;                           
  for (let i = 0; i < x.length; i++){  
    if (typeof(x[i]) === 'number')  sum += x[i];   
    else sumS += +x[i]
  }
  return sum - sumS
}

