// https://www.codewars.com/kata/576bb71bbbcf0951d5000044/train/javascript
function countPositivesSumNegatives(input) {
  if (input === null || input.length === 0 ) return [];
  let cntP = 0;   
  let sumN = 0;   
  for(let i = 0; i < input.length; i++){   
    if (input[i] > 0) cntP ++;
    else sumN += input[i]
  }
  return [cntP, sumN]
}
